<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.garrettchestnut.pushapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SignUp">
        </activity>
        <activity android:name=".LogIn">
        </activity>
        <activity
            android:name=".User"
            android:theme="@android:style/Theme.Translucent">
        </activity>
        <activity
            android:name=".no_team"
            android:label="@string/title_activity_no_team"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".TeamActivity"
            android:label="@string/title_activity_team"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".HasTeamActivity"
            android:label="@string/title_activity_has_team"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>